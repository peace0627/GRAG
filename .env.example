#< .env.example
# Copy this file to .env and fill in your actual values

# === Neo4j Configuration ===
# Neo4j database connection URI
NEO4J_URI=neo4j://localhost:7687
# Neo4j username (default: neo4j)
NEO4J_USER=neo4j
# Neo4j password
NEO4J_PASSWORD=password123

# === Supabase Configuration ===
# Supabase project URL
SUPABASE_URL=https://your-project-url.supabase.co
# Supabase anon/public key
SUPABASE_KEY=your-supabase-anon-key

# === OpenAI Configuration (Optional, for LLM) ===
# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key

# === Qwen2VL Configuration ===
# Qwen2VL API endpoint/base URL
QWEN2VL_BASE_URL=https://api.qwen2vl.com
# Qwen2VL API Key
QWEN2VL_API_KEY=your-qwen2vl-api-key

# === Application Configuration ===
# Debug mode (development: 1, production: 0)
DEBUG=1
# Knowledge Area ID (for knowledge segmentation)
KNOWLEDGE_AREA_ID=default_area
# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# === Optional: Upload/Download paths ===
# Base path for uploaded documents
UPLOAD_BASE_PATH=./uploads
# Maximum file size for uploads (in MB)
MAX_FILE_SIZE_MB=50
